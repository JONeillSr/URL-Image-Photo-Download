            buttonList.forEach(button => {
                const buttonWidth = button.offsetWidth;

                if (buttonWidth > buttonMinWidth) {
                    buttonMinWidth = buttonWidth;
                }
            });

            buttonList.forEach(button => {
                button.style.minWidth = `${buttonMinWidth}px`;
